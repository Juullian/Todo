{
    "name": "Symfony",
    "image": "php:7.4",
    "extensions": ["felixfbecker.php-intellisense"],
    "forwardPorts": [8000],
    "remoteUser": "codespace",
    "remoteEnv": {
      "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/root/.composer/vendor/bin"
    },
    "workspaceMount": "source=/workspace,target=/workspace,type=volume"
  }
  